<?xml version="1.0" encoding="UTF-8"?>

<run>
    <params>
        <!--"/etc/rsyslog.conf /home/my/srdDir /home/my/destDir /home/my/srdDir2 /home/my/destDir2"-->
        <param type="syslog">
            <file>rsyslog.conf</file>
            <items>
                <item>
                    <regex><![CDATA[\n\s?(?<mark>\#+)?\s?(?<facility>\*|\w+)\.(?<level>\*|\w+)\s+((?<tcp>\@@)|(?<udp>\@))(?<ip>\d+\.\d+\.\d+\.\d+)\:(?<port>\d+)]]></regex>
                    <!-- * | auth | authpriv | daemon | user | local0 | local1 | local2 | local3 | local4 | local5 | local6 | local7 -->
                    <facility>snort</facility>
                    <!-- * | emerg | alert | crit | err | warning | notice | info | debug -->
                    <level>alert</level>
                    <proto>udp</proto>
                    <ip>172.16.39.21</ip>
                    <port>514</port>
                </item>
            </items>
        </param>
        <param type="so" file="ld.so.conf">
            <items>
                <item>/usr/lib64/123</item>
            </items>
        </param>
        <param type="ids">
            <items>
                <item type="directory">
                    <src>e:\test\t1</src>
                    <dst>e:\test\t2</dst>
                </item>
                <item type="lib64">
                    <src>e:\test\lib64</src>
                    <dst>e:\test\lib642</dst>
                </item>
            </items>
        </param>
        <param type="shell">
            <items  encode="gb2312">
                <item>
                    <params>
                        <param>cmd</param>
                        <param>/c</param>
                        <param>dir /?</param>
                    </params>
                </item>
                <item>
                    <params>
                        <param>cmd</param>
                        <param>/c</param>
                        <param>dir</param>
                    </params>
                </item>
            </items>
        </param>
    </params>
</run>